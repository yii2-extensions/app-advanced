{
    "name": "yii2-extensions/app-advanced",
    "type": "project",
    "description": "Web Application Advanced",
    "keywords": [
        "application",
        "advanced",
        "web",
        "yii2"
    ],
    "license": "mit",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "npm-asset/fortawesome--fontawesome-free": "^6.4",
        "oomphinc/composer-installers-extender": "^2.0",
        "sjaakp/yii2-icon": "^1.0",
        "yiisoft/config": "^1.3",
        "yiisoft/yii2": "^2.2",
        "yiisoft/yii2-bootstrap5": "^2.0",
        "yiisoft/yii2-symfonymailer": "^3.0"
    },
    "require-dev": {
        "maglnet/composer-require-checker": "^4.6",
        "phpunit/phpunit": "^10.2",
        "yii2-extensions/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "BackEnd\\": "backend",
            "Common\\": "common",
            "Console\\": "console",
            "FrontEnd\\": "frontend/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FrontEnd\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin-file": "/config/config-plugin.php",
        "config-plugin-options": {
            "source-directory": "config"
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "./node_modules/{$name}/": ["type:bower-asset", "type:npm-asset"]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true,
            "yiisoft/config": true
        }
    },
    "scripts": {
        "check-dependencies": "composer-require-checker",
        "phpstan": "phpstan",
        "test": "phpunit"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
