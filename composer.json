{
    "name": "yii2/template",
    "type": "library",
    "description": "_____",
    "keywords": [
        "_____"
    ],
    "license": "mit",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "yiisoft/yii2": "^2.2"
    },
    "require-dev": {
        "maglnet/composer-require-checker": "^4.6",
        "phpunit/phpunit": "^10.2",
        "yii2-extensions/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "yii\\template\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yii\\template\\tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "scripts": {
        "check-dependencies": "composer-require-checker",
        "mutation": "roave-infection-static-analysis-plugin",
        "phpstan": "phpstan",
        "test": "phpunit"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
